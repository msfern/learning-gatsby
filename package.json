{
  "name": "learning-gatsby",
  "version": "1.0.0",
  "private": true,
  "description": "Learning Gatsby",
  "author": "Stephanie Fernandes",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby clean && gatsby develop",
    "start": "gatsby develop",
